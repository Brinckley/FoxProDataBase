SELECT Employee.ename, Employee.esurname;
 FROM ;
     Employee,;
    Deal;
 WHERE (  Employee.id_employee = Deal.id_employee;
   AND   Exists (SELECT Employee.id_employee FROM Employee, Deal WHERE Employee.id_employee = Deal.id_employee AND Deal.dstartdate BETWEEN {^2020/01/01} AND {^2020/03/01}) );
   AND  Deal.dstartdate BETWEEN {^2021/10/09} AND {^2022/12/12}
